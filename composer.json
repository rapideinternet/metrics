{
    "name": "rapide/metrics",
    "description": "A generic metrics library for external services",
    "type": "library",
    "license": "Closed",
    "authors": [
        {
            "name": "Peter Mein",
            "email": "peter@infratron.io"
        }
    ],
    "require": {
        "datadog/php-datadogstatsd": "^1.5",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "myclabs/php-enum": "^1.5",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "1.5.7",
        "phpunit/phpunit": "9.5.20",
        "nikic/php-parser": "4.13.2",
        "symfony/yaml": "^5.0 | ^6.0",
        "friendsofphp/php-cs-fixer": "3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Rapide\\Metrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rapide\\Metrics\\": [
                "tests/"
            ]
        }
    }
}
